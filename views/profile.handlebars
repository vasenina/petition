<form method="POST" id="registerform">
    <h2>Add more information about you</h2>

    <label for="uage">Age: </label>
    <input type="number" id="uage" name="age" />
    <label for="ucity">City: </label>
    <input type="text" id="ucity" name="city" />
    <label for="uurl">Url: </label>
    <input type="text" id="uurl" name="url" />
    <button type="submit">Continue</button>
</form>